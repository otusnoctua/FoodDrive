{% extends "./layout.peb" %}
{% block content %}
    {% for dish in model.order.dishes %}
        <div>
            {{ dish }}
            <a href="/basket/{{model.order.id}}/{{ dish }}/delete" class="button">Удалить блюдо</a>
        </div>
    {% endfor %}

    <a href="/basket/delete/{{model.order.id}}" class="button">Удалить заказ</a>
{% endblock %}