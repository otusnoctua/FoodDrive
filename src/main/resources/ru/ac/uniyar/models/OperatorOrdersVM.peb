{% extends "./layout.peb" %}
{% block content %}

    {% if model.orders is empty %}
        <h3>Сейчас заказов нет</h3>
    {% endif %}

    {% for order in model.orders %}
    <div>
        Заказ номер: {{ order.id }}, Заказчик: {{ order.client.username }}, Время начала: {{ order.startTime }}
        <a href="/orders/{{ order.id }}">Больше</a>
    </div>
    {% endfor %}

{% endblock %}