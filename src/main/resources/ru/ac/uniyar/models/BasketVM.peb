{% extends "./layout.peb" %}
{% block content %}
    {% for order in model.orders %}
        <div>
            Заказ номер: {{ loop.index + 1}}
            <a href="/basket/{{ order.id }}">Больше</a>
            <a href="/basket/{{ order.id }}/accept" class="button">Подтвердить</a>
        </div>
    {% endfor %}
{% endblock %}